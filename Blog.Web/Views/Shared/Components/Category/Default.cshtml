@model List<Blog.Application.ViewModels.Category.CategoryViewModel>

<ul class="list-group">
    @foreach (var category in Model)
    {
        <li class="list-group-item">
            <a asp-action="Index" asp-controller="Category" asp-route-id="@category.Id">
                @category.Name
            </a>
        </li>
    }
</ul>